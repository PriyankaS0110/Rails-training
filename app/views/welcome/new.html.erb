<h1> New Student</h1>

<%= form_with model: @welcomes do |form| %>
  <div>
    <%= form.label :first_name %><br>
    <%= form.text_field :first_name %>
  </div>

  <div>
    <%= form.label :last_name %><br>
    <%= form.text_field :last_name %>
  </div>

  <div>
    <%= form.label :student_email %><br>
    <%= form.email_field :student_email %>
  </div>

    <%= form.submit %>
<% end %>

